sugoi sumarHastaX(limite){

    senpai i = 1-chan
    senpai suma = 0-chan

    kawaii (i <= limite) {

        suma = suma + i -chan
        i = i + 1 -chan
    }

    uwu suma -chan
}

senpai num = 0-chan
baka ("Ingresa un numero: ") -chan
yomu (num) -chan

senpai resultado = sumarHastaX(num) -chan
baka (resultado) -chan

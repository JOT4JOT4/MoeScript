sugoi sumaDeCuadrados(limite) {
    senpai i = 1-chan
    senpai suma = 0-chan

    kawaii (i <= limite) {
        suma = suma + (i * i) -chan
        i = i + 1 -chan
    }
    uwu suma -chan
}

senpai num = 0 -chan
baka ("Ingrese un numero: ")-chan
yomu (num) -chan

senpai resultado = sumaDeCuadrados(num) -chan

baka ("La suma de los cuadrados es: ") -chan
baka (resultado) -chan
